{
  "name": "react-model-binding",
  "version": "0.4.1",
  "description": "Bind React Components to backboney models/collections",
  "main": "react-model-binding.js",
  "scripts": {
    "test": "jest",
    "test-debug": "node-debug node_modules/jest-cli/bin/jest.js -i",
    "test-watch": "jest --watch",
    "travis": "eslint *.js test/*.js{x} fixtures/*.js{x} && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nathanstitt/react-model-binding.git"
  },
  "keywords": [
    "react",
    "events",
    "backbone",
    "ampersand"
  ],
  "author": "Nathan Stitt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nathanstitt/react-model-binding/issues"
  },
  "peerDependencies": {
    "react": ">=0.14.2",
    "react-dom": ">=0.14.2",
    "lodash": "^4.0"
  },
  "homepage": "https://github.com/nathanstitt/react-model-binding#readme",
  "devDependencies": {
    "ampersand-state": "^5.0.2",
    "eslint-plugin-react": "^5.2.2",
    "jest-cli": "^18.0",
    "lodash": "^4.0",
    "react": ">=0.14.2",
    "react-addons-test-utils": ">=0.14.2",
    "react-dom": ">=0.14.2"
  },
  "jest": {
    "verbose": true,
    "notify": true,
    "testRegex": "tests/.*\\-test\\.(js)$",
    "moduleFileExtensions": [
      "js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/"
